{
    "presets": [
        "stage-0",
        [
            "env",
            {
                "modules": false
            }
        ],
        "babel-preset-minify"
    ],
    "plugins": [
        [
            "transform-runtime",
            {
                "polyfill": false,
                "regenerator": true
            }
        ],
        [
            "babel-plugin-syntax-dynamic-import"
        ]
    ],
    "compact": true,
    "comments": false
}
